<?xml version="1.0" encoding="utf-8"?>
<groupBy>

  <group DisplayName="Visual Studio Interop" AttractConsumers="False">
    <patterns>
      <pattern RegEx=".*\.Interop\..*"/>
    </patterns>
  </group>
  
  <group DisplayName="Team Foundation.Common" AttractConsumers="False">
    <patterns>
      <pattern RegEx="^microsoft\.teamfoundation\.common\.dll"/>
    </patterns>
  </group>

  <group DisplayName="DSL Toolkit" AttractConsumers="False">
    <patterns>
      <pattern RegEx="^microsoft\.visualstudio\.modeling.*"/>
    </patterns>
  </group>

  <group DisplayName="Team Foundation Client">
    <patterns>
      <pattern RegEx="^microsoft\.teamfoundation\.versioncontrol\..*"/>
      <pattern RegEx=".*\.teamfoundation.*\.workitemtracking\..*"/>
    </patterns>
  </group>

  <group DisplayName="VSTS Test">
